{
	"type": "template",
	"template_name": "cnn.tpl",
	"parameters": [
		"name",
		"pre_method",
		"post_method",
		"inputs",
		"outputs"
	],
	"template": {
		"name": "${name}",
		"type": "workflow",
		"inputs": ["${inputs}"],
		"outputs": ["${outputs}"],
		"workflow": [
		    {
				"type": "node",
				"method_type": "${pre_method}",
				"unique_name": "pre",
				"method_config_file": "@include: ./cnn_config.json",
				"inputs": ["inputs"],
				"outputs": [
					"pre_out0",
					"pre_out1"
				]
			},
			{
				"type": "node",
				"method_type": "CNNMethod",
				"unique_name": "cnn",
				"inputs": [
					"pre_out0",
					"pre_out1"
				],
				"outputs": [
					"cnn_out0"
				]
			},
			{
				"type": "node",
				"method_type": "${post_method}",
				"unique_name": "post",
				"inputs": ["cnn_out0"],
				"outputs": ["outputs"]
			}
		]
	}
}
